{
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[css]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "never"
    }
  },
  "cSpell.words": ["biomejs", "stylelint"],
  "editor.codeActionsOnSave": {
    "quickfix.biome": true,
    "source.organizeImports.biome": true
  }
}
